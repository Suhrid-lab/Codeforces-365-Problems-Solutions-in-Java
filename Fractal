import java.io.*;
import java.util.*;

public class Fractal {
    public static void main(String[] args) throws IOException {
        BufferedReader br = new BufferedReader(new FileReader("input.txt"));
        PrintWriter pw = new PrintWriter(new FileWriter("output.txt"));
        
        StringTokenizer st = new StringTokenizer(br.readLine());
        int n = Integer.parseInt(st.nextToken());
        int k = Integer.parseInt(st.nextToken());
        
        char[][] pattern = new char[n][n];
        for (int i = 0; i < n; i++) {
            pattern[i] = br.readLine().toCharArray();
        }
        
        int size = (int)Math.pow(n, k);
        char[][] result = generateFractal(pattern, n, k);
        
        for (int i = 0; i < size; i++) {
            pw.println(new String(result[i]));
        }
        
        br.close();
        pw.close();
    }
    
    private static char[][] generateFractal(char[][] pattern, int n, int k) {
        int size = (int)Math.pow(n, k);
        char[][] result = new char[size][size];
        
        // Initialize with white
        for (int i = 0; i < size; i++) {
            Arrays.fill(result[i], '.');
        }
        
        // Generate fractal
        for (int i = 0; i < size; i++) {
            for (int j = 0; j < size; j++) {
                int x = i;
                int y = j;
                boolean black = false;
                
                // Check through all k levels
                for (int level = 0; level < k; level++) {
                    int blockSize = (int)Math.pow(n, k - level - 1);
                    int blockX = x / blockSize;
                    int blockY = y / blockSize;
                    
                    if (pattern[blockX % n][blockY % n] == '*') {
                        black = true;
                        break;
                    }
                    
                    // Move to next level
                    x %= blockSize;
                    y %= blockSize;
                }
                
                if (black) {
                    result[i][j] = '*';
                }
            }
        }
        
        return result;
    }
}
