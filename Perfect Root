import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int t = scanner.nextInt();
        
        for (int i = 0; i < t; i++) {
            int n = scanner.nextInt();
            
            // Output n distinct numbers starting from 1, but in any order
            // We'll output numbers from 1 to n for simplicity
            for (int j = 1; j <= n; j++) {
                System.out.print(j);
                if (j < n) {
                    System.out.print(" ");
                }
            }
            System.out.println();
        }
        
        scanner.close();
    }
}
