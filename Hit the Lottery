import java.io.*;

public class Main {
    public static void main(String[] args) throws Exception {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        long n = Long.parseLong(br.readLine().trim());

        long bills = 0;
        int[] d = {100, 20, 10, 5, 1};

        for (int x : d) {
            bills += n / x;
            n %= x;
        }

        System.out.print(bills);
    }
}
