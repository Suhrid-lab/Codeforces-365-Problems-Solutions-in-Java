# Read input
gets stdin n

# Function to check if a number is prime
proc isPrime {x} {
    if {$x < 2} { return 0 }
    if {$x == 2} { return 1 }
    if {$x % 2 == 0} { return 0 }
    for {set i 3} {$i * $i <= $x} {incr i 2} {
        if {$x % $i == 0} { return 0 }
    }
    return 1
}

# Find left prime
set a $n
while {![isPrime $a]} {
    incr a -1
}

# Find right prime
set b $n
while {![isPrime $b]} {
    incr b
}

# Output result
puts "$a $b"
