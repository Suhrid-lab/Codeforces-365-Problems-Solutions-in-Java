import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.next();

        int idx = s.indexOf('0');
        if (idx != -1) {
            System.out.print(s.substring(0, idx) + s.substring(idx + 1));
        } else {
            System.out.print(s.substring(0, s.length() - 1));
        }
    }
}
